{
  "expo": {
    "name": "expo-idnow-auto-ident-example",
    "slug": "expo-idnow-auto-ident-example",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "expo.modules.idnowautoident.example",
      "infoPlist": {
        "NSCameraUsageDescription": "We need access to the camera to verify your identity.",
        "NSPhotoLibraryUsageDescription": "We need access to the photo library to upload identification documents."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "expo.modules.idnowautoident.example"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "../app.plugin.js",
      [
        "expo-build-properties",
        {
          "android": {
            "extraMavenRepos": [
              "https://raw.githubusercontent.com/idnow/de.idnow.android.sdk/master"
            ]
          }
        }
      ]
    ]
  }
}
